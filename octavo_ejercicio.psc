Algoritmo octavo_ejercicio
	Definir num1, num2, suma, resta, multi Como Entero;
	Definir div Como Real
	Escribir "Dato 1:";
	Leer num1;
	Escribir "Dato 2:";
	Leer num2;
	suma = num1 + num2;
	resta = num1 - num2;
	multi = num1 * num2;
	div = num1 / num2;
	Escribir "Suma=" suma;
	Escribir "Resta=" resta;
	Escribir "Multiplicacion=" multi;
	Escribir "Division=" div;
FinAlgoritmo
